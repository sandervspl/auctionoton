{
  "name": "auctionoton-server",
  "version": "1.10.0",
  "description": "Code for auctionoton-server.",
  "private": true,
  "main": "src/Server.ts",
  "author": "Sander Vispoel <sandervspl@github>",
  "license": "ISC",
  "scripts": {
    "dev": "NODE_ENV=development APP_ENV=development ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/Server.ts",
    "build": "npm run clean && tsc",
    "start": "NODE_ENV=production APP_ENV=production node -r ./tsconfig-paths-bootstrap.js dist/Server.js",
    "clean": "rimraf dist/",
    "lint": "eslint --ext .ts src/",
    "typecheck": "tsc --noEmit",
    "test": "npx concurrently 'npm run lint' 'npm run typecheck'"
  },
  "dependencies": {
    "@nestjs/common": "8.2.4",
    "@nestjs/config": "1.1.5",
    "@nestjs/core": "8.2.4",
    "@nestjs/platform-express": "8.2.4",
    "@nestjs/throttler": "2.0.0",
    "@project/constants": "*",
    "@project/types": "*",
    "@project/validation": "*",
    "blizzapi": "2.1.0",
    "cache-manager": "3.6.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "compression": "1.7.4",
    "cors": "^2.8.5",
    "htmlparser2": "7.2.0",
    "immer": "9.0.7",
    "kleur": "4.1.4",
    "lodash": "4.17.21",
    "nestjs-pino": "2.4.0",
    "node-cache": "5.1.2",
    "node-fetch": "^2.6.6",
    "pino": "7.6.2",
    "pino-pretty": "7.3.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "7.5.1",
    "tsconfig-paths": "3.12.0"
