{
  "name": "auctionoton-extension",
  "version": "3.0.2",
  "description": "Code for auctionoton-extension.",
  "private": true,
  "main": "index.js",
  "author": "Sander Vispoel <sandervspl@github>",
  "license": "ISC",
  "scripts": {
    "lint": "eslint ./src --ext .ts,.tsx",
    "build": "NODE_ENV=production node esbuild.config.js",
    "dev": "node esbuild.config.js",
    "postbuild": "cd dist; zip -D -r ../auctionoton-$npm_package_version.zip ./*; zip -d ../auctionoton-$npm_package_version.zip *.DS_Store",
    "preversion": "tsc --noEmit && npm run lint",
    "version": "npx json -I -f src/manifest.json -e \"this.version='$npm_package_version'\"; git add .",
    "postversion": "git push && git push --tags && npm run build && npm run mozilla",
    "mozilla": "zip -r auctionoton-$npm_package_version-source.zip ./src config.js defines.d.ts global.d.ts package.json package-lock.json README.md tsconfig.json webpack.config.js",
    "tc": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/css": "11.7.1",
    "@project/constants": "*",
    "@project/types": "*",
    "@project/validation": "*",
    "dayjs": "1.10.7",
    "immer": "9.0.7",
    "lodash": "4.17.21",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.22.5",
    "react-query": "3.34.7",
    "tsafe": "0.9.0",
    "typed-query-selector": "2.6.1",
    "w3c-keys": "1.0.3",
    "zustand": "3.6.8"
  },
  "devDependencies": {
    "@types/chrome": "0.0.175",
    "@types/firefox-webext-browser": "94.0.1",
    "@types/lodash": "4.14.178",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "esbuild": "0.14.11",
    "esbuild-plugin-svgr": "1.0.0",
    "eslint": "8.6.0",
    "eslint-config-react-app": "7.0.0",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "fs-extra": "10.0.0",
    "node-notifier": "10.0.0",
    "rimraf": "3.0.2",
    "typescript": "4.5.4"
  }
}
