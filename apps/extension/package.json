{
  "name": "extension",
  "version": "3.2.6",
  "description": "Code for auctionoton-extension.",
  "private": true,
  "main": "index.js",
  "author": "Sander Vispoel <sandervspl@github>",
  "license": "ISC",
  "scripts": {
    "lint": "eslint ./src --ext .ts,.tsx --fix",
    "build": "NODE_ENV=production node esbuild.config.js",
    "dev": "rm -rf ./dist && npx concurrently 'node esbuild.config.js' 'npx tailwindcss -i ./src/style.css -o ./dist/style.css' 'npx tailwindcss -i ./src/modules/browserAction/style.scss -o ./dist/style_form.css'",
    "postbuild": "cd dist; zip -D -r ../auctionoton-$npm_package_version.zip ./*",
    "preversion": "tsc --noEmit",
    "version": "changeset version && npx json -I -f src/manifest.json -e \"this.version='$npm_package_version'\"; git add .",
    "postversion": "git push && git push --tags && npm run build && npm run mozilla",
    "mozilla": "zip -r auctionoton-$npm_package_version-source.zip ./src config.js defines.d.ts global.d.ts package.json package-lock.json README.md tsconfig.json webpack.config.js",
    "tc": "tsc --noEmit"
  },
  "dependencies": {
    "@project/constants": "*",
    "@project/types": "*",
    "@project/utils": "*",
    "@project/validation": "*",
    "axios": "^1.1.3",
    "axios-rate-limit": "^1.3.0",
    "classnames": "^2.3.2",
    "dayjs": "1.11.5",
    "immer": "9.0.15",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.34.2",
    "react-query": "3.39.2",
    "tsafe": "1.0.1",
    "typed-query-selector": "2.7.0",
    "valtio": "^1.6.4",
    "w3c-keys": "1.0.3"
  },
  "devDependencies": {
    "@types/chrome": "0.0.196",
    "@types/firefox-webext-browser": "94.0.1",
    "@types/lodash": "4.14.184",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "autoprefixer": "^10.4.8",
    "esbuild": "0.15.6",
    "esbuild-plugin-svgr": "1.0.1",
    "eslint": "8.23.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.4",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-tailwindcss": "^3.6.1",
    "fs-extra": "10.1.0",
    "node-notifier": "10.0.1",
    "postcss-import": "15.0.0",
    "rimraf": "3.0.2",
    "tailwindcss": "3.1.8",
    "typescript": "4.8.2"
  }
}
